{
  "name": "template-snowpack-mithril-ts-mocha",
  "version": "0.0.1",
  "description": "Template for Mithril. Build with Snowpack. Write on Typescript. Tests with Mocha.",
  "scripts": {
    "start": "snowpack dev",
    "prebuild": "tsc && npm run lint",
    "build": "snowpack build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "stylelint": "stylelint  **/*.css",
    "stylelint:fix": "stylelint **/*.css --fix",
    "test": "mocha --recursive **/*.spec.mjs",
    "storybook": "cross-env TS_NODE_PROJECT=tsconfig.storybook.json start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazerlelutin/template-snowpack-mithril-ts-mocha.git"
  },
  "keywords": [
    "mithril",
    "mocha",
    "typscript",
    "snowpack"
  ],
  "author": {
    "email": "b@bouteiller.contact",
    "name": "Kazerlelutin",
    "url": "https://bouteiller.contact"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kazerlelutin/template-snowpack-mithril-ts-mocha/issues"
  },
  "homepage": "https://github.com/kazerlelutin/template-snowpack-mithril-ts-mocha#readme",
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-docs": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/mithril": "^6.2.9",
    "@types/mithril": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-plugin-postcss-modules": "^2.0.0",
    "eslint-plugin-storybook": "^0.5.5",
    "jsdom": "^19.0.0",
    "mithril-query": "^4.0.1",
    "mocha": "^9.1.4",
    "snowpack": "^3.8.8",
    "stylelint": "^14.2.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "dependencies": {
    "mithril": "^2.0.4"
  }
}
